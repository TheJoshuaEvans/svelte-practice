{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --host 0.0.0.0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:up": "npm --prefix ../iac run docker:up",
    "docker:down": "npm --prefix ../iac run docker:down",
    "attach": "npm --prefix ../iac run attach:frontend"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "eslint": "^8.10.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "rollup": "^2.69.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4"
  },
  "dependencies": {
    "dompurify": "^2.3.6",
    "marked": "^4.0.12",
    "sirv-cli": "^2.0.2"
  }
}
